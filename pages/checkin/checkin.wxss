}
.image_header {
  width: 100%;
  position: fixed;
  top: 0px;
  z-index: 99;
}

.checkin_title {
  text-align: center;
  font-size: 50rpx;
  margin-top: -50rpx;
}
.title {
  font-size: 40rpx;
  margin: 10rpx 0rpx;
}
.checkin_content {
  padding: 10px 30px;
}
.checkin_btn {
  padding: 20rpx 60rpx 20rpx 60rpx; 
  text-align: center;
}
.checkin_btn .btn {
  background: #e20000; 
  border-radius: 200rpx; 
  width: 100%; 
  margin: 0 auto; 
  text-align: center; 
  color: white; 
  padding: 20rpx 0rpx;
}
.question {
  padding:20rpx 60rpx;
  color: #e20000;
  text-decoration: underline;
  font-family: "黑体";
}


.image_footer {
  width: 100%;
  position: fixed;
  bottom: 0px;
}

